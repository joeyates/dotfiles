// Place your key bindings in this file to override the defaults
[
  {
    "key": "ctrl+p",
    "command": "editor.action.triggerSuggest",
    "when": "editorHasCompletionItemProvider && !suggestWidgetVisible && textInputFocus && !editorReadonly"
  },
  {
    "key": "ctrl+n",
    "command": "editor.action.triggerSuggest",
    "when": "editorHasCompletionItemProvider && !suggestWidgetVisible && textInputFocus && !editorReadonly"
  }
]
